#include "stdafx.h"

int main(int argc, char* argv[])
{
	int a[10],n,i,k,x=0,y=0;
	int b[10],m;
	printf("enter n=");
		scanf("%d",&n);
	printf("enter m=");
		scanf("%d",&m);
	for(i=0;i<n;i++)
        scanf("%d",&a[i]);
	for(i=0;i<m;i++)
        scanf("%d",&b[i]);

	for(i=0;i<n;i++){
		for(k=0;k<m;k++){
			if(a[i]==b[k]) x++;}
         	else	 if(x==0) printf("%d",a[i]);}
	for(i=0;i<m;i++){
		for(k=0;k<n;k++){
			if(b[i]==a[k]) y++;}
	else if(y==0) printf("%d"ï¼Œb[i]);
		 }
	return 0;
}
