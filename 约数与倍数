#include<stdio.h>
int main()
{
    int m,n,a,b;
    scanf("%d",&m);
    scanf("%d",&n);
    
    if(m>n)
    {
    {
    {b=m;
     if(b%n==0)
    printf("最小公倍数是%d\n",b);
     else
    {do
    b=b+1;
    while(b%m!=0 || b%n!=0);
    printf("最小公倍数是%d\n",b);}
    }
    }
    }
    if(m>n)
    {
    {a=n;
     if(m%a==0)
    printf("最大公约数是%d\n",a);
     else
    {do
    a=a-1;
    while(m%a!=0 || n%a!=0);
    printf("最大公约数是%d\n",a);}
    }
