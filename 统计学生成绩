#include<stdio.h>

int main()
{
    int n,i,A,B,C,D,E;
    int s;
    printf("学生人数");
    scanf("%d",&n);
    A=0,B=0,C=0,D=0,E=0;
    
    for(i=1;i<=n;i=i+1)
    {  printf("学生成绩");
       scanf("%d",&s);
    switch(s/10)
    {
    case 10: A=A+1;break;
    case 9 : A=A+1;break;
    case 8 : B=B+1;break;
    case 7 : C=C+1;break;
    case 6 : D=D+1;break;
    default: E=E+1;break;
    }
    }
    printf("A等级人数=%d\nB等级人数=%d\nC等级人数=%d\nD等级人数=%d\nE等级人数=%d\n",A,B,C,D,E);
    return 0;
}
